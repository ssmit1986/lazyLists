Cell[TextData[{
 "Use ",
 Cell[BoxData[
  FormBox[
   StyleBox[
    RowBox[{"TakeWhile", "[", "lz", "]"}],
    FontWeight->"Bold"], TraditionalForm]],ExpressionUUID->
  "5ef15816-5a91-457d-87f1-6bfc8c368bce"],
 " or ",
 Cell[BoxData[
  FormBox[
   StyleBox[
    RowBox[{"Take", "[", 
