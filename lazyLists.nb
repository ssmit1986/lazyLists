   RowBox[{"Fibonacci", "[", 
    RowBox[{"Range", "[", "1000", "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"lazyList", "[", "fibList", "]"}], "//", 
  "ByteCount"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"lazyList", "[", 
   RowBox[{"Hold", "[", "fibList", "]"}], "]"}], "//", 
